package Lincalc;
import java.util.*;

public class LinCalcDavid {

	public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String expression;
        expression = in.nextLine();
	
		
		String[] split = (expression.length);
		
		for (i = 0; i < split.length; i++) {
			System.out.println(split);
		}
		
	}

}
